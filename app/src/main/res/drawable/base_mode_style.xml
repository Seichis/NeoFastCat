<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="oval"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#20534e" />
            <gradient  android:angle="-90"  android:startColor="#0D47A1" android:endColor="#2962FF"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="oval"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#20534e" />
            <solid  android:color="#125156"/>
        </shape>
    </item>
    <item >
        <shape android:shape="oval"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#1976D2" />
            <gradient  android:angle="-90"  android:startColor="#42A5F5" android:endColor="#82B1FF" />
        </shape>
    </item>
</selector>